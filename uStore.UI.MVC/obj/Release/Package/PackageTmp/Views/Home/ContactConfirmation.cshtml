@model uStore.UI.MVC.Models.ContactViewModel

@{
    ViewBag.Title = "Contact Confirmation";
}


<div class="row container">
    <div class="col-sm-4">
        <div class="contact-info">
            <h2 class="title text-center">@ViewBag.Title</h2>
        </div>
    </div>
    <div class="col-sm-8">
        <!--Response-->
        <h5 class="confirmationMsg">
            Thank you for contacting us, @Model.Name!  We will get back to you as soon as possible.
        </h5>
    </div>
</div>



